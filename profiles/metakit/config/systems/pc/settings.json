{
  "romsets": {
    "exodos": {
      "resources": {
        "config": {
          "source": "file://$EXODOS_V6_HOME/eXo/eXoDOS/!dos/{machine_sourcefile}/dosbox.conf",
          "download": "$RETROKIT_HOME/config/systems/pc/conf/{machine}.conf",
          "target": "$RETROKIT_HOME/config/systems/pc/conf/{machine}.conf",
          "install": {
            "ignore_missing_configs": "$EXODOS_IGNORE_MISSING_CONFIGS",
            "overwrite_target": true
          }
        }
      }
    }
  },
  "manuals": {
    "websites": [
      {
        "name": "exodos",
        "source": "https://archive.org/download/exov5_2/Content/XODOSMetadata.zip/",
        "match": "(?P<path>Manuals/MS-DOS/[^\"<]+)"
      },
      {
        "name": "internetarchive",
        "source": "internetarchive",
        "filter": {
          "include": "ms.?dos"
        }
      },
      {
        "name": "exodosv6",
        "source": "$EXODOS_V6_MANUALS_LIST",
        "match": "^(?P<path>.*/(?P<name>(?P<group>[^/]+) \\([0-9]{4}\\))\\.[a-zA-Z]+$)",
        "required": false
      }
    ]
  }
}
