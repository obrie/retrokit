{
  "manuals": {
    "websites": [
      {
        "name": "segaretro",
        "shared": true,
        "source": "https://segaretro.org/Special:NewFiles?like=&user=&mediatype%5B%5D=OFFICE&start={year}-{month}-{day}&end=&wpFormIdentifier=specialnewimages&limit=5000&offset=",
        "match": "(?P<path>/File:[^\" ]*Manual[^\" ]*\\.pdf)",
        "filter": {
          "include": "MD",
          "exclude": "_(SG-?1000|SMS|32X|MegaCD|MCD|SegaCD|SCD|GG|Saturn|Sat|SS|DC|PC|PS2)_"
        }
      },
      {
        "name": "internetarchive",
        "source": "internetarchive",
        "filter": {
          "include": "megadrive|sega.*genesis"
        }
      }
    ]
  }
}
