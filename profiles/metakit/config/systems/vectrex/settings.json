{
  "manuals": {
    "websites": [
      {
        "name": "digitpress",
        "source": "http://www.digitpress.com/library/manuals/vectrex/index.html",
        "match": "(?P<path>[^\"]+\\.(pdf|txt))[^>]+>(?P<name>[^<]+)"
      },
      {
        "name": "internetarchive",
        "source": "internetarchive",
        "filter": {
          "include": "vectrex"
        }
      }
    ]
  }
}
