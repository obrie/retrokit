{
  "manuals": {
    "websites": [
      {
        "name": "emuland",
        "source": "https://www.emu-land.net/library/manuals/gba/all",
        "match": "(?P<path>library/manuals.+id=[0-9]+)[^>]+>(?P<name>[^<]+)",
        "base_href": "https://www.emu-land.net/"
      },
      {
        "name": "internetarchive",
        "source": "internetarchive",
        "filter": {
          "include": "gba|(gb|game.?boy).?advance"
        }
      }
    ]
  }
}
