{
  "manuals": {
    "websites": [
      {
        "name": "digitpress",
        "source": "http://www.digitpress.com/library/manuals/intv/index.html",
        "match": "(?P<path>[^\"]+\\.(pdf|txt))[^>]+>(?P<name>[^<]+)"
      },
      {
        "name": "annarchive",
        "source": "https://annarchive.com/intellivision_manuals.html",
        "match": "(?P<path>[^\"]+pdf)"
      },
      {
        "name": "internetarchive",
        "source": "internetarchive",
        "filter": {
          "include": "intellivision"
        }
      }
    ]
  }
}
