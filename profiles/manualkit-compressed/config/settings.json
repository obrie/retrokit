{
  "manuals": {
    "archive": {
      "version": "compressed",
      "url": "https://archive.org/download/retrokit-manuals/{system}/{system}-original.zip/{name} ({languages}).pdf",
      "processed": false
    },
    "paths": {
      "download": "{base}/.files/{name} ({languages}).pdf",
      "postprocess": "{base}/.files-compressed/{name} ({languages}).pdf",
      "meta": "{base}/.files-compressed/{name} ({languages}).json"
    },
    "keep_downloads": true,
    "postprocess": {
      "clean": {
        "enabled": false
      },
      "ocr": {
        "enabled": false
      },
      "mutate": {
        "enabled": false
      }
    }
  }
}