{
  "system": "channelf",
  "metadata": {
    "path": "$RETROKIT_HOME/data/channelf.json"
  },
  "overlays": {
    "default": "https://github.com/thebezelproject/bezelprojectSA-ChannelF/raw/master/retroarch/overlay/Fairchild-Channel-F.png",
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-ChannelF",
        "path": "retroarch/overlay/GameBezels/ChannelF"
      },
      {
        "repo": "thebezelproject/bezelprojectSA-ChannelF",
        "path": "retroarch/overlay/GameBezels/ChannelF"
      }
    ]
  },
  "emulators": {
    "lr-freechaf": {
      "default": true,
      "core_name": "freechaf",
      "library_name": "FreeChaF"
    }
  },
  "bios": {
    "url": "$BIOS_URL/libretro/Fairchild Channel F",
    "dir": "$HOME/RetroPie/BIOS",
    "files": {
      "sl31253.bin": "{url}/sl31253.bin",
      "sl31254.bin": "{url}/sl31254.bin"
    }
  },
  "romsets": {
    "nointro": {
      "protocol": "http",
      "discovery": {
        "type": "internetarchive",
        "urls": [
          "$ROMSET_NOINTRO_URL"
        ],
        "match": "Fairchild - Channel F"
      },
      "resources": {
        "dat": {
          "source": "file://$RETROKIT_HOME/tmp/No-Intro Love Pack (PC XML).zip",
          "target": "$RETROKIT_HOME/cache/nointro/Fairchild - Channel F (Parent-Clone).dat",
          "install": {"action": "zip_extract", "file": ".*/Fairchild - Channel F.*\\.dat"}
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "$HOME/RetroPie/roms/channelf/.nointro/{machine}.zip",
          "xref": "$HOME/RetroPie/roms/channelf/.nointro/.xrefs/{machine_id}.zip"
        }
      },
      "auth": "internetarchive"
    }
  },
  "roms": {
    "id": "crc",
    "priority": {
    },
    "filters": {
    },
    "favorites": {
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/channelf", "filters": {}}
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
