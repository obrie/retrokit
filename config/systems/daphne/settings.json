{
  "system": "daphne",
  "metadata": {
    "path": "$HOME/retrokit/data/daphne.json"
  },
  "scraper": {
    "import_dat_titles": false
  },
  "overlays": {
    "default": "https://github.com/thebezelproject/bezelprojectSA-Daphne/raw/master/retroarch/overlay/Daphne.png",
    "target": "/opt/retropie/emulators/hypseus/bezels",
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-Daphne",
        "path": "retroarch/overlay/GameBezels/Daphne"
      },
      {
        "repo": "thebezelproject/bezelprojectSA-Daphne",
        "path": "retroarch/overlay/GameBezels/Daphne"
      }
    ],
    "lightgun_border": {
      "offset_x": 226
    }
  },
  "emulators": {
    "hypseus": {
      "default": true,
      "supports_overlays": true
    },
    "actionmax": {
      "build": "source"
    }
  },
  "romsets": [
    {
      "name": "laserdisc",
      "protocol": "http",
      "url": "$ROMSET_LASERDISC_URL",
      "resources": {
        "machine": {
          "source": "{url}/roms.zip/roms/{machine}.zip",
          "target": "$HOME/RetroPie/roms/daphne/.roms/{machine}.zip"
        },
        "disk": {
          "source": "{url}/{machine_description}.zip",
          "download": "$HOME/RetroPie/roms/daphne/.laserdiscs/{machine}.zip",
          "target": "$HOME/RetroPie/roms/daphne/.laserdiscs/{machine}.daphne",
          "install": {"action": "zip_extract", "include_parent": false},
          "file_identifier": "name"
        }
      },
      "datlist": {
        "ace": {"description": "Space Ace", "disks": [{"name": "ace"}]},
        "astron": {"description": "Astron Belt", "disks": [{"name": "astron"}]},
