{
  "system": "wonderswan",
  "metadata": {
    "path": "$RETROKIT_HOME/data/wonderswan.json"
  },
  "overlays": {
    "default": "https://github.com/thebezelproject/bezelprojectSA-WonderSwan/raw/master/retroarch/overlay/Bandai-WonderSwan-Horizontal.png",
    "vertical": "https://github.com/thebezelproject/bezelprojectSA-WonderSwan/raw/master/retroarch/overlay/Bandai-WonderSwan-Vertical.png",
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-WonderSwan",
        "path": "retroarch/overlay/GameBezels/WonderSwan"
      },
      {
        "repo": "thebezelproject/bezelprojectSA-WonderSwan",
        "path": "retroarch/overlay/GameBezels/WonderSwan"
      }
    ]
  },
  "emulators": {
    "lr-beetle-wswan": {
      "default": true,
      "core_name": "wswan",
      "library_name": "Beetle WonderSwan",
      "gamestate": [
      ]
    }
  },
  "romsets": {
    "nointro": {
      "protocol": "http",
      "discovery": {
        "type": "internetarchive",
        "urls": [
          "$ROMSET_NOINTRO_URL/no-intro_romsets_files.xml"
        ],
        "match": "no-intro romsets/Bandai - WonderSwan \\("
      },
      "resources": {
        "dat": {
          "source": "file://$RETROKIT_HOME/tmp/No-Intro Love Pack (PC XML).zip",
          "target": "$RETROKIT_HOME/cache/nointro/Bandai - WonderSwan (Parent-Clone).dat",
          "install": {"action": "zip_extract", "file": ".*/Bandai - WonderSwan \\(.*\\.dat", "overwrite_target": true}
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "$HOME/RetroPie/roms/wonderswan/.nointro/{machine}.zip",
          "xref": "$HOME/RetroPie/roms/wonderswan/.nointro/.xrefs/{machine_id}.zip"
        }
      },
      "auth": "internetarchive"
    }
  },
  "roms": {
    "id": "crc",
    "priority": {
    },
    "filters": {
    },
    "favorites": {
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/wonderswan", "filters": {}}
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
