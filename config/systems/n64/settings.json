{
  "system": "n64",
  "metadata": {
    "path": "$RETROKIT_HOME/data/n64.json",
    "defaults": {
      "emulation": {
        "rating": 0
      }
    }
  },
  "overlays": {
    "default": "https://github.com/thebezelproject/bezelprojectSA-N64/raw/master/retroarch/overlay/Nintendo-64.png",
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-N64",
        "path": "retroarch/overlay/GameBezels/N64"
      },
      {
        "repo": "thebezelproject/bezelprojectSA-N64",
        "path": "retroarch/overlay/GameBezels/N64"
      }
    ]
  },
  "emulators": {
    "mupen64plus": {
      "names": [
        "mupen64plus-gles2n64",
        "mupen64plus-gles2rice",
        "mupen64plus-GLideN64",
        "mupen64plus-GLideN64-highres"
      ],
      "gamestate": [
        "/opt/retropie/configs/n64/mupen64plus/save/*"
      ]
    },
    "lr-mupen64plus": {
      "default": true,
      "core_name": "mupen64plus",
      "library_name": "Mupen64Plus GLES3"
    },
    "lr-mupen64plus-next": {
      "core_name": "mupen64plus-next",
      "library_name": "Mupen64Plus-Next"
    },
    "lr-parallel-n64": {
      "core_name": "parallel-n64",
      "library_name": "ParaLLEl N64"
    }
  },
  "cheats": {
    "names": [
      "Nintendo - Nintendo 64"
    ]
  },
  "romsets": {
    "nointro": {
      "discovery": {
        "type": "internetarchive",
        "urls": [
          "$ROMSET_NOINTRO_URL"
        ],
        "match": {
          "url": "Nintendo - Nintendo 64 \\(BigEndian\\)"
        }
      },
      "resources": {
        "dat": {
          "source": "file://$RETROKIT_HOME/tmp/No-Intro Love Pack (PC XML).zip",
          "target": "$RETROKIT_HOME/cache/nointro/Nintendo - Nintendo 64 (BigEndian) (Parent-Clone).dat",
          "install": {"action": "zip_extract", "file": ".*/Nintendo - Nintendo 64 \\(BigEndian\\).*\\.dat"}
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "download": "$HOME/RetroPie/roms/n64/.nointro/{machine}.zip",
          "target": "$HOME/RetroPie/roms/n64/.nointro/{machine}.z64",
          "xref": "$HOME/RetroPie/roms/n64/.nointro/.xrefs/{machine_id}.z64",
          "install": {"action": "zip_extract", "file": ".*\\.z64"}
        }
      },
      "auth": {
        "type": "internetarchive"
      }
    }
  },
  "roms": {
    "id": "crc",
    "priority": {
    },
    "filters": {
    },
    "favorites": {
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/n64", "filters": {}}
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
