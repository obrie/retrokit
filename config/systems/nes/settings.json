{
  "system": "nes",
  "metadata": {
    "path": "$RETROKIT_HOME/data/nes.json"
  },
  "overlays": {
    "default": "https://github.com/thebezelproject/bezelprojectSA-NES/raw/master/retroarch/overlay/Nintendo-Entertainment-System.png",
    "repos": [
      {
        "repo": "thebezelproject/bezelproject-NES",
        "path": "retroarch/overlay/GameBezels/NES"
      },
      {
        "repo": "thebezelproject/bezelprojectSA-NES",
        "path": "retroarch/overlay/GameBezels/NES"
      }
    ]
  },
  "emulators": {
    "lr-fceumm": {
      "default": true,
      "core_name": "fceumm",
      "library_name": "FCEUmm"
    }
  },
  "cheats": {
    "names": [
      "Nintendo - Nintendo Entertainment System"
    ]
  },
  "romsets": {
    "nointro": {
      "discovery": {
        "type": "internetarchive",
        "urls": [
          "$ROMSET_NOINTRO_URL"
        ],
        "match": {
          "url": "Nintendo - Nintendo Entertainment System.*\\[headered]"
        }
      },
      "resources": {
        "dat": {
          "source": "file://$RETROKIT_HOME/tmp/No-Intro Love Pack (PC XML).zip",
          "target": "$RETROKIT_HOME/cache/nointro/Nintendo - Nintendo Entertainment System (Parent-Clone).dat",
          "install": {"action": "zip_extract", "file": ".*/Nintendo - Nintendo Entertainment System \\(Headerless\\).*\\.dat"}
        },
        "machine": {
          "source": "{discovery_url}/{machine}.zip",
          "target": "$HOME/RetroPie/roms/nes/.nointro/{machine}.zip",
          "xref": "$HOME/RetroPie/roms/nes/.nointro/.xrefs/{machine_id}.zip",
          "file_identifier": "name"
        }
      }
    }
  },
  "roms": {
    "id": "crc",
    "priority": {
    },
    "filters": {
    },
    "favorites": {
    },
    "dirs": [
      {"path": "$HOME/RetroPie/roms/nes", "filters": {}}
    ],
    "files": {
      "machine": {"target": "{dir}/{machine_filename}"}
    }
  }
}
